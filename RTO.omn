Class: rehabilitation-therapy-ontology:PrimaryProgressiveAphasia

    Annotations: 
        rehabilitation-therapy-ontology:derived-from "https://github.com/MohammadHalawani/PhD-datasets/terms-subclasses.csv"@en,
